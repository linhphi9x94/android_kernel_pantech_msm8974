	memory_hole: qcom,msm-mem-hole {
        	compatible = "qcom,msm-mem-hole";
              qcom,memblock-remove = <0x7b00000 0x5f00000
                                          0xdc00000 0x2300000>; /* Address and Size of Hole */

	};

    i2c_2: i2c@f9924000{
        cell-index = <2>;
        compatible = "qcom,i2c-qup";
        reg = <0xf9924000 0x1000>;
          #address-cells = <1>;
          #size-cells = <0>;
        reg-names = "qup_phys_addr";
        interrupts = <0 96 0>;
      interrupt-names = "qup_err_intr";
        qcom,i2c-bus-freq = <400000>;
        qcom,i2c-src-freq = <50000000>;

        atmel_mxt_fw30@4a {
            compatible = "atmel,atmel_mxt_540s";
            reg = <0x4a>;
            interrupt-parent = <&msmgpio>;
            interrupts = <61 0x2>;
        };
    };

  i2c_9: i2c@f9965000 {
		cell-index = <9>;
		compatible = "qcom,i2c-qup";
		reg = <0Xf9965000 0x1000>;
  #address-cells = <1>;
  #size-cells = <0>;
		reg-names = "qup_phys_addr";
		interrupts = <0 103 0>;
		interrupt-names = "qup_err_intr";
		qcom,i2c-bus-freq = <400000>;
		qcom,i2c-src-freq = <50000000>;
		qcom,scl-gpio = <&msmgpio 52 0>;
		qcom,sda-gpio = <&msmgpio 51 0>;		
		lp5523_led_driver@32 {
			compatible = "ti,lp5523_led_driver";
			reg = <0x32>;
			lp5523,clock_mode = <1>;
			lp5523,led_en_gpio = <16>;
			lp5523,max_chan = <9>;
			lp5523,led_current = <40>;
			lp5523,max_current = <250>;
			lp5523,trg_touchkey-backlight = <4 5>;
      
		};			
	};

	i2c@f9967000 {
		isa1200@48 {
			status = "okay";
			reg = <0x48>;
			vcc_i2c-supply = <&pm8941_s3>;
			compatible = "imagis,isa1200";
			label = "vibrator";
			imagis,chip-en;
			imagis,need-pwm-clk;
			imagis,ext-clk-en;
			imagis,hap-en-gpio = <&msmgpio 86 0x00>;
			imagis,max-timeout = <15000>;
			imagis,pwm-div = <256>;
			imagis,mode-ctrl = <2>;
			imagis,regulator {
				regulator-name = "vcc_i2c";
				regulator-min-microvolt = <1800000>;
				regulator-max-microvolt = <1800000>;
				regulator-max-microamp = <9360>;
			};
		};
	};
	/* NFC */
	i2c@f9928000 {
			cell-index = <6>; 
			compatible = "qcom,i2c-qup"; 
			reg = <0xf9928000 0x1000>; 
			#address-cells = <1>;
			#size-cells = <0>;
			reg-names = "qup_phys_addr"; // Name of QUP_BASE
			interrupts = <0 100 0>;
			interrupt-names = "qup_err_intr"; 
			qcom,i2c-bus-freq = <400000>;
			qcom,i2c-src-freq = <50000000>; 
			pn547@28 {
			compatible = "nxp,pn547";
			reg = <0x28>;
			interrupt-parent = <&msmgpio>;
			interrupts = <59 0x2>;
			nxp,irq-gpio = <&msmgpio 59 0x00>;
			interrupt-names = "irq_gpio";
			nxp,sda-gpio = <&msmgpio 29 0>; /* NFC_I2C_SDA */
			nxp,scl-gpio = <&msmgpio 30 0>; /* NFC_I2C_SCL */
			nxp,ven-gpio = <&msmgpio 50 0>; /* NFC_EXT_EN_MSM */
			nxp,firm-gpio = <&pm8941_gpios  32 0>; /* NFC_F/W_D/L_2 */
		};
	};

